<p id="notice"><%= notice %></p>


  <!-- Table -->
<table class="table table-striped">
  <thead>
    <tr>
      <th>Name</th>
      <th>Description</th>
      <th>Url</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody">
    <% @startups.each do |startup| %>
      <tr>
        <td><%= link_to startup.name, startup %></td>
        <td><%= startup.description %></td>
        <td><a href="<%= startup.url %>"><%= startup.url %></a></td>
      </tr>
    <% end %>
  </tbody>
</table>




