<p id="notice"><%= notice %></p>

<table class="table table-striped">
  <thead>
    <tr>
      <th>Name</th>
      <th>Description</th>
      <th>Url</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
      <tr>
        <td><%= @startup.name %></td>
        <td><%= @startup.description %></td>
        <td><a href="<%= @startup.url %>"><%= @startup.url %></a></td>
      </tr>
  </tbody>
</table>

<p class="one">
<%= link_to 'Edit', edit_startup_path(@startup), :class => 'btn btn-default' %>
<%= link_to 'Back', startups_path, :class => 'btn btn-default' %> 
<%= link_to 'Delete', @startup, method: :delete, data: { confirm: 'Are you sure?' }, :class => 'btn btn-danger'%>
</p>